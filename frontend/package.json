{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-run": "npm run build && makers copyrun",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@iconify-prerendered/vue-ion": "^0.17.1672653773",
    "@iconify-prerendered/vue-material-symbols": "^0.17.1670570880",
    "@iconify-prerendered/vue-mdi": "^0.17.1672388948",
    "@iconify-prerendered/vue-tabler": "^0.17.1672389125",
    "@types/three": "^0.146.0",
    "three": "^0.147.0",
    "vue": "^3.4.23",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.16"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@iconify/json": "^2.2.201",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@playwright/test": "^1.43.1",
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.19.31",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/compiler-sfc": "^3.4.23",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.5.3",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.8.27"
  }
}
